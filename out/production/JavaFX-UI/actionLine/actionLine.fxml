<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="33.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="actionLine.ActionLineController">
   <children>
      <TextField fx:id="cellIdSelection" alignment="CENTER" editable="false" minHeight="26.0" prefHeight="26.0" promptText="selected cell">
         <HBox.margin>
            <Insets left="25.0" right="40.0" top="3.0" />
         </HBox.margin>
      </TextField>
      <TextField fx:id="originalValueBox" alignment="CENTER" onAction="#handleUpdateValueTextFieldAction" minHeight="26.0" prefHeight="26.0" promptText="original value">
         <HBox.margin>
            <Insets left="-35.0" right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="updateValue" contentDisplay="TOP" onAction="#handleUpdateValueAction" prefHeight="26.0" text="update value" textAlignment="JUSTIFY">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label text="last version updated: " textAlignment="CENTER">
         <HBox.margin>
            <Insets top="3.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="showLastVersion" editable="false" minHeight="26.0" prefHeight="26.0">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="versionSelector" onAction="#handleVersionSelectorAction" prefHeight="26.0" text="version selector" textAlignment="CENTER">
         <HBox.margin>
            <Insets top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="lastVersionButton" onAction="#handleLastVersionButton" prefHeight="26.0" text="get back to last version">
         <HBox.margin>
            <Insets left="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</HBox>
