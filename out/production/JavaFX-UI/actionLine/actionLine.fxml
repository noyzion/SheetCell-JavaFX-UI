<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="33.0" prefWidth="965.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="actionLine.ActionLineController">
   <children>
      <TextField fx:id="cellIdSelection" alignment="CENTER" editable="false" minHeight="-Infinity" prefHeight="26.0" prefWidth="107.0" promptText="selected cell">
         <HBox.margin>
            <Insets left="25.0" right="40.0" top="3.0" />
         </HBox.margin>
      </TextField>
      <TextField fx:id="originalValueBox" alignment="CENTER" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#handleUpdateValueTextFieldAction" prefHeight="26.0" prefWidth="224.0" promptText="original value">
         <HBox.margin>
            <Insets left="-35.0" right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="5.0" />
         </padding>
      </TextField>
      <Button fx:id="updateValue" contentDisplay="TOP" minHeight="-Infinity" mnemonicParsing="false" onAction="#handleUpdateValueAction" prefHeight="26.0" prefWidth="116.0" text="update value" textAlignment="JUSTIFY">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="13.0" />
         </font>
         <padding>
            <Insets bottom="0.2" />
         </padding>
      </Button>
      <Label prefHeight="26.0" prefWidth="116.0" text="last version updated: " textAlignment="CENTER">
         <HBox.margin>
            <Insets top="3.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="showLastVersion" editable="false" minHeight="-Infinity" prefHeight="26.0" prefWidth="26.0">
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="versionSelector" minHeight="-Infinity" mnemonicParsing="false" onAction="#handleVersionSelectorAction" prefHeight="26.0" prefWidth="128.0" text="version selector" textAlignment="CENTER">
         <HBox.margin>
            <Insets top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="0.2" />
         </padding>
      </Button>
      <Button fx:id="lastVersionButton" alignment="TOP_CENTER" minHeight="-Infinity" mnemonicParsing="false" onAction="#handleLastVersionButton" prefHeight="26.0" prefWidth="186.0" text="get back to last version">
         <HBox.margin>
            <Insets left="5.0" top="3.0" />
         </HBox.margin>
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="0.2" />
         </padding>
      </Button>
   </children>
</HBox>
