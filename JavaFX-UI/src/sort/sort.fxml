<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="260.0" prefWidth="155.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sort.SortController">
   <children>
      <Label prefHeight="29.0" prefWidth="36.0" text="sort:">
         <font>
            <Font size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="60.0" top="5.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="27.0" prefWidth="152.0">
         <children>
            <Label prefHeight="29.0" prefWidth="47.0" text="from:">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Label>
            <Label prefHeight="29.0" prefWidth="47.0" text="to:">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="42.0" prefWidth="151.0">
         <children>
            <ComboBox fx:id="startCell" editable="true" prefHeight="31.0" prefWidth="66.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="endCell" editable="true" prefHeight="31.0" prefWidth="67.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
      </HBox>
      <Label text="sort by the columns:">
         <VBox.margin>
            <Insets bottom="4.0" left="18.0" />
         </VBox.margin></Label>
      <VBox fx:id="checkboxContainer" spacing="5.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="55.0" />
         </padding>
      </VBox>
      <Button fx:id="sortButton" mnemonicParsing="false" onAction="#handleSortAction" text="sort">
         <VBox.margin>
            <Insets left="53.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="cancelSortButton" mnemonicParsing="false" onAction="#handleCancelSortAction" text="cancel sort">
         <VBox.margin>
            <Insets left="30.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
