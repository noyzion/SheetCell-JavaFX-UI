<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="59.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="actionLine.ActionLineController">
   <content>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="53.0" prefWidth="759.0">
         <center>
            <HBox prefHeight="66.0" prefWidth="743.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="cellIdSelection" alignment="CENTER" editable="false" minHeight="-Infinity" prefHeight="26.0" prefWidth="95.0" promptText="selected cell">
                     <HBox.margin>
                        <Insets left="20.0" right="40.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="originalValueBox" alignment="CENTER" editable="false" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="224.0" promptText="original value">
                     <HBox.margin>
                        <Insets bottom="20.0" left="-35.0" right="5.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
                  <Button fx:id="updateValue" contentDisplay="TOP" minHeight="-Infinity" mnemonicParsing="false" onAction="#handleUpdateValueAction" prefHeight="26.0" prefWidth="116.0" text="update value" textAlignment="JUSTIFY">
                     <HBox.margin>
                        <Insets bottom="20.0" right="5.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                  <Label prefHeight="26.0" prefWidth="116.0" text="last version updated: " textAlignment="CENTER">
                     <HBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="showLastVersion" editable="false" minHeight="-Infinity" prefHeight="26.0" prefWidth="26.0">
                     <HBox.margin>
                        <Insets bottom="20.0" right="5.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="versionSelector" minHeight="-Infinity" mnemonicParsing="false" onAction="#handleVersionSelectorAction" prefHeight="26.0" prefWidth="128.0" text="version selector" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
